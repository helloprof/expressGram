<div class="container" style="padding-top:60px">
  <h1>Profiles</h1>
  <a href="/profiles/add" class="btn btn-dark">Add Profile</a>
  <table class="table">
    <thead class="thead-dark">
      <tr>
        <th scope="col">ID</th>
        <th scope="col">Profile</th>
        <th scope="col">Delete</th>
      </tr>
    </thead>
    <tbody>
      {{#each data}}
      <tr>
        <td>{{profileID}}</td>
        <td>{{profile}}</td>
        <td><a class="btn btn-danger" href="/profiles/delete/{{profileID}}">Delete</a></td>
      </tr>
      {{else}}
      <tr>
        <td>NO DATA!</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>